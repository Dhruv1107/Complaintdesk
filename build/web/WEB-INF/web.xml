<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>FirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePass</servlet-name>
        <servlet-class>ChangePass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgotServlet</servlet-name>
        <servlet-class>ForgotServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetPass</servlet-name>
        <servlet-class>SetPass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreComServlet</servlet-name>
        <servlet-class>StoreComServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowComServlet</servlet-name>
        <servlet-class>ShowComServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IChangePass</servlet-name>
        <servlet-class>IChangePass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IShowComServlet</servlet-name>
        <servlet-class>IShowComServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResolveComplaint</servlet-name>
        <servlet-class>ResolveComplaint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IResComServlet</servlet-name>
        <servlet-class>IResComServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/FirstServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePass</servlet-name>
        <url-pattern>/ChangePass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgotServlet</servlet-name>
        <url-pattern>/ForgotServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetPass</servlet-name>
        <url-pattern>/SetPass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreComServlet</servlet-name>
        <url-pattern>/StoreComServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowComServlet</servlet-name>
        <url-pattern>/ShowComServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IChangePass</servlet-name>
        <url-pattern>/IChangePass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IShowComServlet</servlet-name>
        <url-pattern>/IShowComServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResolveComplaint</servlet-name>
        <url-pattern>/ResolveComplaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IResComServlet</servlet-name>
        <url-pattern>/IResComServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
